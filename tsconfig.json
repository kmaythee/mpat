{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@app/*": [
                "./app/*"
            ]
        },
        "outDir": "./www/out-tsc",
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "removeComments": true,
        "suppressImplicitAnyIndexErrors": true,
        "lib": [
            "es2016",
            "dom"
        ],
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [ ]
    },
    "angularCompilerOptions": {
        "genDir": ".",
        "debug": true
    },
    "awesomeTypescriptLoaderOptions": {
        "useWebpackText": true
    },
    "exclude": [
        "node_modules",
        "www"
    ],
    "compileOnSave": false,
    "buildOnSave": false
}